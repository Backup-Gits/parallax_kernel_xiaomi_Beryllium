for FILE in $INITD/$FSPEC; do
  sleep 1
  LogMSG "Running init on sdcard/d8g/init"
  sh $FILE >/dev/null
done;

# Install APK
if [ ! -d $D8G_DIR/apk ]; then
	LogMSG "--------------------------"
else
	LogMSG " "
	LogMSG "Installer"
	LogMSG "--------------------------"
	LogMSG "Checking Installer"
	if [ "$(ls -A /$D8G_DIR/apk)" ]; then
		LogMSG "  - Starting Installer"
		cd $D8G_DIR/apk
		chmod 777 *;
		for apk in *.apk; do
			LogMSG "    * Install $apk"
			pm install -r $apk >/dev/null;
			#sleep 5
			#rm $apk
		done;
		#rm -fr $D8G_DIR/apk
	else
		LogMSG "  - Installer not running"
		if [ -d $D8G_DIR/apk ]; then
			rm -fr $D8G_DIR/apk
		fi
	fi
	LogMSG "--------------------------"
fi

